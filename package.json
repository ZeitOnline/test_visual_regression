  },
  "homepage": "https://github.com/ZeitOnline/test_visual_regression#readme",
  "devDependencies": {
    "backdrop": "0.0.3",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0"
  }
}
